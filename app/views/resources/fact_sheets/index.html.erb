<p id="notice"><%= notice %></p>

<h1>Fact Sheets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fact_sheets.each do |fact_sheet| %>
      <tr>
        <td><%= link_to 'Show', fact_sheet %></td>
        <td><%= link_to 'Edit', edit_fact_sheet_path(fact_sheet) %></td>
        <td><%= link_to 'Destroy', fact_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fact Sheet', new_fact_sheet_path %>
