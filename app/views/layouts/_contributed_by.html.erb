<div class="col-sm-4">
  <div class="contributed-by">
    <% if resource.contributed_by %>
      <h4 class="contributed-by__title">Contributed by</h4>
      <ul class="contributed-by__list">
        <% resource.contributed_by.split("\n").each do |contributor| %>
          <li><h5 class="contributed-by__name"><%= contributor %></h5></li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>


