<ol class="breadcrumb">
  <% breadcrumbs(request).each do |crumb| %>
    <% if !is_a_number(crumb[:title]) %>
      <% unless crumb == breadcrumbs(request).last %>
        <li><%= link_to crumb[:title], crumb[:url] %></li>
      <% else %>
        <li class="active"><%= crumb[:title] %></li>
      <% end %>
    <% end %>
  <% end %>
</ol>
