<section class="section" aria-label="Content">
  <div class="strip strip--brand-color strip--home">
    <div class="container">
      <div class="row">
        <div class="col-sm-8 strip__left">
          <div class="jumbotron">
            <h1 class="title--strip">Page Not Found</h1>
            <p class="intro">
              Sorry we couldn't find the page you were looking for, it's possible that functionality has not been completed yet.
            </p>
          </div>

        </div>
        <div class="col-sm-4 strip__about-this-site">
          <h3 class="title">About this site and how to use it</h3>
          <%= render 'layouts/embedded_video', video_url: 'https://www.youtube.com/embed/cRskjqpgSNs?rel=0' %>
        </div>
      </div>

      <div class="row">
        <div class="col-sm-12 strip__left">
          <%= render 'layouts/search_widget', search_path: search_path %>
        </div>
      </div>
    </div>
  </div>
  <%= render 'layouts/topics' %>
  <%= render 'layouts/questions_strip' %>
</section>
